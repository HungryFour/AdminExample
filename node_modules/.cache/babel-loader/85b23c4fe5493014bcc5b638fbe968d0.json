tItem('user')\n\n  if (url === LOGINURL) {\n    sessionStorage.removeItem('user')\n  }\n\n  // 判断是否登录\n  if (!user && url !== LOGINURL) {\n    next({ path: LOGINURL })\n  } else {\n    next()\n  }\n})\n\nexport default router\n"]}]}