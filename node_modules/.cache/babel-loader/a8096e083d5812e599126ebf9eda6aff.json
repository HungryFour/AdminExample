B,UAAMK,MAAM,GAAGC,QAAQ,CAACC,YAAT,IAAyBD,QAAQ,CAACE,eAAlC,IAAqDF,QAAQ,CAACG,kBAA7E,CADmB,CAEnB;;AACA,UAAIJ,MAAJ,EAAY;AACV,YAAMK,KAAK,GAAGJ,QAAQ,CAACK,cAAT,IAA2BL,QAAQ,CAACM,sBAApC,IAA8DN,QAAQ,CAACO,mBAAvE,IAA8FP,QAAQ,CAACQ,gBAArH;AACAJ,QAAAA,KAAK,IAAIA,KAAK,CAACK,IAAN,CAAWT,QAAX,CAAT;AACD,OAHD,MAGO;AACL,YAAMU,MAAM,GAAGV,QAAQ,CAACW,eAAxB;AACA,YAAMC,IAAI,GAAGF,MAAM,CAACG,iBAAP,IAA4BH,MAAM,CAACI,uBAAnC,IAA8DJ,MAAM,CAACK,oBAArE,IAA6FL,MAAM,CAACM,mBAAjH;AACAJ,QAAAA,IAAI,IAAIA,IAAI,CAACH,IAAL,CAAUC,MAAV,CAAR;AACD;AACF;;;;;;SAxBkBzB,K","sourcesContent":["export default class Utils {\n  // 当前日期\n  static todayDate () {\n    const dt = new Date()\n    const y = dt.getFullYear()\n    const m = dt.getMonth() + 1\n    const d = dt.getDate()\n    const w = dt.getDay()\n    const weeks = ['天', '一', '二', '三', '四', '五', '六']\n    return `${y} 年 ${m.toString().padStart(2, '0')} 月 ${d.toString().padStart(2, '0')} 日 星期${weeks[w]}`\n  }\n\n  // 浏览器全屏\n  static fullScreen () {\n    const isFull = document.isFullScreen || document.mozIsFullScreen || document.webkitIsFullScreen\n    // 判断是否全屏\n    if (isFull) {\n      const close = document.exitFullscreen || document.webkitCancelFullScreen || document.mozCancelFullScreen || document.msExitFullscreen\n      close && close.call(document)\n    } else {\n      const docElm = document.documentElement\n      const open = docElm.requestFullScreen || docElm.webkitRequestFullScreen || docElm.mozRequestFullScreen || docElm.msRequestFullscreen\n      open && open.call(docElm)\n    }\n  }\n}\n"]}]}