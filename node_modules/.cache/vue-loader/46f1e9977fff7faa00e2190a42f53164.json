;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuOA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA","file":"Goods.vue","sourceRoot":"src/views","sourcesContent":["<template>\n  <div class=\"article-box\">\n\n    <div style=\"display: flex; justify-content: space-between; width: 100%\">\n      <el-form ref=\"form\" :inline=\"true\" label-position=\"left\" size=\"mini\">\n        <el-form-item label=\"系列名称:\">\n          <el-select clearable filterable placeholder=\"请选择\" v-model=\"activity_select_condition\">\n            <el-option\n              v-for=\"item in activity_selects\"\n              :key=\"item.id\"\n              :label=\"item.title\"\n              :value=\"item.title\"\n            ></el-option>\n          </el-select>\n        </el-form-item>\n\n        <el-form-item label=\"上市状态:\">\n          <el-select clearable filterable placeholder=\"请选择\" v-model=\"activity_status_selects_condition\">\n            <el-option\n              v-for=\"item in activity_status_selects\"\n              :key=\"item.label\"\n              :label=\"item.label\"\n              :value=\"item.value\"\n            ></el-option>\n          </el-select>\n        </el-form-item>\n\n        <el-form-item>\n          <el-button type=\"primary\" @click=\"table_data\">查询</el-button>\n        </el-form-item>\n      </el-form>\n      <el-button size=\"mini\" style=\"width: 100px; height: 30px\" type=\"primary\" @click=\"add_new\">发送所选订单</el-button>\n    </div>\n\n    <el-table\n      :data=\"articles\"\n      style=\"width: 100%\">\n      <el-table-column\n        type=\"selection\"\n        width=\"55\">\n      </el-table-column>\n      <el-table-column\n        prop=\"title\"\n        label=\"系列名称\"\n        width=\"280\">\n      </el-table-column>\n      <el-table-column\n        prop=\"imageurl\"\n        label=\"系列图片\"\n        width=\"130\">\n        <template slot-scope=\"scope\">\n          <el-image\n            style=\"width: 100px; height: 100px\"\n            :src=\"scope.row.image\"\n            :preview-src-list=\"[scope.row.image]\"\n            fit=\"contain\">\n          </el-image>\n        </template>\n      </el-table-column>\n      <el-table-column\n        prop=\"author\"\n        label=\"作者\"\n        width=\"280\">\n      </el-table-column>\n      <el-table-column\n        sortable\n        prop=\"date\"\n        label=\"上市日期\"\n        width=\"280\">\n      </el-table-column>\n      <el-table-column\n        label=\"状态\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.status ? '已发布' : '未发布' }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" fixed=\"right\" width=\"150\">\n        <template slot-scope=\"scope\">\n          <el-button\n            size=\"mini\"\n            type=\"primary\"\n            plain\n            @click=\"handleEdit(scope.$index, scope.row)\">编辑\n          </el-button>\n          <el-button\n            size=\"mini\"\n            type=\"danger\"\n            @click=\"handleDelete(scope.$index, scope.row)\">删除\n          </el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n    <el-pagination\n      background\n      :page-sizes=\"[10, 20, 30, 50]\"\n      :page-size=\"20\"\n      layout=\"total, sizes, prev, pager, next, jumper\"\n      :total=\"400\">\n    </el-pagination>\n    <el-dialog title=\"编辑\" :visible.sync=\"articleFormVisible\" top=\"10vh\" @close=\"resetForm('articleForm')\">\n      <el-form :model=\"article\" ref=\"articleForm\" :rules=\"rules\">\n        <el-form-item label=\"标题\" prop=\"title\" label-width=\"50px\">\n          <el-input v-model=\"article.title\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"作者\" label-width=\"50px\">\n          <el-input v-model=\"article.author\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"日期\" label-width=\"50px\">\n          <el-date-picker\n            v-model=\"article.date\"\n            type=\"date\"\n            value-format=\"yyyy-MM-dd\"\n            placeholder=\"选择日期\">\n          </el-date-picker>\n        </el-form-item>\n        <el-form-item label=\"状态\" label-width=\"50px\">\n          <el-switch v-model=\"article.status\" active-color=\"#13ce66\"\n                     inactive-color=\"#ff4949\"\n                     :active-value=\"1\"\n                     :inactive-value=\"0\"></el-switch>\n        </el-form-item>\n        <el-form-item>\n          <quill-editor ref=\"TextEditor\"\n                        v-model=\"article.content\"\n                        :options=\"editorOption\">\n          </quill-editor>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"articleFormVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"editArticle('articleForm')\">确 定</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\n  // 引入富文本编辑器所需样式\n  import 'quill/dist/quill.core.css'\n  import 'quill/dist/quill.snow.css'\n  import 'quill/dist/quill.bubble.css'\n\n  import { quillEditor } from 'vue-quill-editor'\n\n  export default {\n    data () {\n      return {\n        articles: [],\n        article: {\n          id: '',\n          date: '',\n          title: '',\n          author: '',\n          content: '',\n          status: 0\n        },\n        articleFormVisible: false,\n        rowIndex: 9999,\n        editorOption: {\n          modules: {\n            toolbar: [\n              ['bold', 'italic', 'underline', 'strike', 'link'],\n              ['image', 'clean']\n            ]\n          },\n          placeholder: '请输入文章内容...',\n          theme: 'snow'\n        },\n        rules: {\n          title: [\n            { required: true, message: '请输入标题', trigger: 'blur' },\n            { min: 5, max: 25, message: '长度在 5 到 25 个字符', trigger: 'blur' }\n          ]\n        }\n      }\n    },\n    components: {\n      quillEditor\n    },\n    mounted () {\n      this.getArticles()\n    },\n    methods: {\n      getArticles () {\n        this.loading = true\n        this.$http('/api/articles').then((res) => {\n          this.articles = res.data\n        }).catch((err) => {\n          console.error(err)\n        })\n      },\n      handleEdit (index, row) {\n        this.article = Object.assign({}, row)\n        this.articleFormVisible = true\n        this.rowIndex = index\n      },\n      editArticle (formName) {\n        this.$refs[formName].validate((valid) => {\n          if (valid) {\n            this.articles.splice(this.rowIndex, 1, this.article)\n            this.articleFormVisible = false\n            this.$message({\n              type: 'success',\n              message: '修改成功!'\n            })\n          }\n        })\n      },\n      handleDelete (index, row) {\n        this.$confirm(`确定删除新闻 【${row.title}】 吗?`, '提示', {\n          confirmButtonText: '确定',\n          cancelButtonText: '取消',\n          type: 'warning'\n        }).then(() => {\n          this.articles.splice(index, 1)\n          this.$message({\n            type: 'success',\n            message: '删除成功!'\n          })\n        }).catch(() => {\n          console.log('取消删除')\n        })\n      },\n      resetForm (formName) {\n        this.$refs[formName].clearValidate()\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  .article-box {\n    width: 100%;\n\n    .el-pagination {\n      margin-top: 20px;\n    }\n\n    .quill-editor {\n      height: 300px;\n      margin-bottom: 20px;\n    }\n  }\n</style>\n"]}]}